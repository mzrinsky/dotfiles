set color_win_inactive_cur_sel_bg=235
set color_win_inactive_cur_sel_fg=248
set color_win_inactive_cur_sel_attr=default

set color_win_inactive_sel_bg=235
set color_win_inactive_sel_fg=248
set color_win_inactive_sel_attr=default

set color_win_sel_bg=197
set color_win_sel_fg=255


set color_titleline_bg=233
set color_titleline_fg=197
set color_titleline_attr=default

set color_statusline_bg=232
set color_statusline_fg=197
set color_statusline_attr=default

set color_cmdline_bg=233
set color_cmdline_fg=255
set color_cmdline_attr=default

